<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER" maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" spacing="20.0" type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="lblSokoban" text="SOKOBAN">
         <font>
            <Font name="Segoe UI Black" size="48.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <Label fx:id="lblSpelerNaam" text="Speler">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <Button fx:id="btnSpeelSpel" mnemonicParsing="false" onAction="#btnSpeelSpelAfhandeling" prefHeight="40.0" prefWidth="176.0" text="Speel spel">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
      <Button fx:id="btnMaakNieuwSpel" mnemonicParsing="false" onAction="#btnMaakNieuwSpelAfhandeling" prefHeight="40.0" prefWidth="176.0" text="Maak nieuw spel">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
      <Button fx:id="btnWijzigSpel" mnemonicParsing="false" onAction="#btnWijzigSpelAfhandeling" prefHeight="40.0" prefWidth="176.0" text="Wijzig spel">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
      <Button fx:id="btnAfsluiten" mnemonicParsing="false" onAction="#btnAfsluitenAfhandeling" prefHeight="40.0" prefWidth="176.0" text="Afsluiten">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="100.0" left="100.0" right="100.0" top="100.0" />
   </padding>
</fx:root>
